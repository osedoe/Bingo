{
  "devDependencies": {
    "autoprefixer": "^9.1.5",
    "cssnano": "^4.1.4",
    "lodash": "^4.17.21",
    "postcss-clean": "^1.1.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-font-magician": "^2.2.1",
    "postcss-mixins": "^6.2.0",
    "postcss-preset-env": "^6.0.10",
    "stylelint": "^9.6.0"
  },
  "name": "bingo",
  "description": "Template to jump-start a project with Parcel.",
  "version": "1.0.0",
  "main": "index.js",
  "dependencies": {},
  "scripts": {
    "dev": "parcel serve src/index.html --open",
    "watch": "parcel watch src/index.html",
    "build": "parcel build src/index.html -d build --public-url /Bingo/",
    "clean:cache": "rm -rf .cache dist build",
    "clean:all": "rm -rf node_modules package-lock.json .cache dist build",
    "deploy": "git subtree push --prefix build origin gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/osedoe/Bingo.git"
  },
  "keywords": [
    "parcel"
  ],
  "author": "Jose Diaz",
  "license": "ISC"
}
